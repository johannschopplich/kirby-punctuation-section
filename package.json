{
  "name": "kirby-punctuation-section",
  "type": "module",
  "version": "3.0.14",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all --execute \"npx zx scripts/pre-release.mjs\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.0",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "kirbyup": "^3.1.13",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.11",
    "zx": "^8.1.6"
  }
}
