{
  "private": true,
  "version": "2.1.10",
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "dev": "kirbyup src/panel/index.js --watch",
    "serve": "kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "lint": "eslint src/panel",
    "lint:fix": "eslint src/panel --fix",
    "format": "prettier --write \"src/panel/**/*\"",
    "release": "npm run build && npx -y bumpp --all"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "kirbyup": "next",
    "prettier": "^2.8.7"
  }
}
