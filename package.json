{
  "name": "kirby-punctuation-section",
  "type": "module",
  "version": "3.0.14",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all --execute \"npx zx scripts/pre-release.mjs\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "autoprefixer": "^10.4.21",
    "bumpp": "^10.2.2",
    "eslint": "^9.33.0",
    "kirbyup": "^3.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "zx": "^8.8.0"
  }
}
