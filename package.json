{
  "name": "kirby-punctuation-section",
  "type": "module",
  "version": "3.0.12",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all --execute \"npx zx scripts/pre-release.mjs\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.3",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "kirbyup": "^3.1.6",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4"
  }
}
